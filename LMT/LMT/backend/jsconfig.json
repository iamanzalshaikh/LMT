{
  "compilerOptions": {
    "module": "ESNext",
    "target": "ES2020",
    "baseUrl": "./",
    "paths": {
      "@controllers/*": ["src/controller/*"],
      "@routes/*": ["src/routes/*"],
      "@models/*": ["src/models/*"],
      "@utils/*": ["src/utils/*"]
    }
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules"]
}
